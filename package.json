{
  "name": "captureease",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.6.0",
    "@simplewebauthn/browser": "^13.1.2",
    "@simplewebauthn/server": "^13.1.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "i18next": "^25.4.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "react": "^18.3.1",
    "react-big-calendar": "^1.15.0",
    "react-calendar": "^5.0.0",
    "react-dom": "^18.3.1",
    "react-firebase-hooks": "^5.1.1",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.30.2",
    "resend": "^4.7.0",
    "slick-carousel": "^1.8.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prestart": "npm cache clean --force",
    "test:unit": "react-scripts test --testPathPattern=src/tests/components --watchAll=false",
    "test:integration": "react-scripts test --testPathPattern=src/tests/integration --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false --testResultsProcessor=jest-sonar-reporter",
    "test:aggressive": "npm run test:coverage && npm run test:integration && npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "jest-sonar-reporter": "^2.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js",
      "!src/**/*.test.js",
      "!src/**/*.spec.js",
      "!src/tests/**/*",
      "!src/**/__tests__/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}